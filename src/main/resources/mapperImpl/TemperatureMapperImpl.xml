<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myhdas_backend.mapper.TemperatureMapper">

    <select id="queryId" parameterType="java.lang.Integer" resultType="com.myhdas_backend.bean.Temperature">
        select * from temperature where id = #{id}
    </select>

    <select id="queryList" parameterType="java.lang.Integer" resultType="com.myhdas_backend.bean.Temperature">
        select * from temperature limit #{count};
    </select>

    <select id="queryResult" parameterType="java.lang.Integer" resultType="java.lang.Integer">
        select temperature from temperature limit #{count};
    </select>

</mapper>