<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myhdas_backend.mapper.UserMapper">

    <select id="queryName" parameterType="java.lang.String" resultType="com.myhdas_backend.bean.User">
        select * from user where uname = #{name};
    </select>

    <select id="queryPhone" parameterType="java.lang.Integer" resultType="com.myhdas_backend.bean.User">
        select * from user where phone = #{phone};
    </select>

    <select id="queryList"  resultType="com.myhdas_backend.bean.User">
        select * from user;
    </select>

</mapper>